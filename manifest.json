{
  "name": "Maten Store",
  "short_name": "Maten",
  "start_url": "/index.html",
  "id": "/index.html",
  "display": "standalone",
  "display_override": ["window-controls-overlay"],
  "scope": "/",
  "dir": "rtl",
  "background_color": "#f0f2f5",
  "theme_color": "#1a365d",
  "description": "فرۆشگای MATEN بۆ فرۆشتنی کاڵا جۆراوجۆرەکان. باشترین نرخ و کوالێتی.",
  "lang": "ku",
  "orientation": "portrait",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "prefer_related_applications": false,
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.example.app1",
      "id": "com.example.app1"
    }
  ],
  "scope_extensions": [
    { "origin": "*.r70732950-web.github.io/rekany/" },
    { "origin": "r70732950-web.github.io/rekany/" }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+maten",
      "url": "/index.html?product=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/index.html",
      "accept": {
        "image/*": [".png", ".gif", ".jpeg", ".jpg"]
      }
    }
  ],
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "share_target": {
    "action": "/index.html",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "widgets": [
    {
      "name": "New Products",
      "short_name": "New",
      "description": "See the latest products from Maten Store",
      "tag": "new-products",
      "template": "new_products_template",
      "ms_ac_template": "new_products_template.json",
      "data": "new_products_data.json",
      "screenshots": [
        {
          "src": "images/screenshots/widget_screenshot.png",
          "sizes": "600x800",
          "type": "image/png"
        }
      ],
      "icons": [
        {
          "src": "images/icons/widget_icon.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ],
      "auth": false,
      "update": 86400
    }
  ],
  "icons": [
    {
      "src": "images/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "images/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "images/screenshots/screenshot1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Home Screen"
    },
    {
      "src": "images/screenshots/screenshot2.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Product Details"
    },
    {
      "src": "images/screenshots/wide_screenshot1.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Desktop Home Screen"
    }
  ],
  "shortcuts": [
    {
      "name": "سەبەتەی کڕین",
      "short_name": "سەبەتە",
      "description": "بینینی کاڵاکانی ناو سەبەتە",
      "url": "/index.html#cart",
      "icons": [{ "src": "images/icons/cart.png", "sizes": "96x96" }]
    }
  ],
  "categories": ["shopping", "lifestyle"]
}
